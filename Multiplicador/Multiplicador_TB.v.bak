module Multiplicador_TB();
	
	reg St, rst, clk;
	reg[15:0] Multiplicador, Multiplicando;
	wire Idle, Done;
	wire [31:0] Produto;
	
	Multiplicador DUT (
		.St(St),
		.rst(rst),
		.clk(clk),
		.Multiplicador(Multiplicador),
		.Multiplicando(Multiplicando),
		.Idle(Idle), 
		.Done(Done),
		.Produto(Produto)
	);

	initial begin
		forever #5 clk = ~clk;
	end
	
	initial begin
		St  = 0;
		rst = 0;
		clk   = 0;
		Multiplicador = 5000;
		Multiplicando = 6000;
		#10;
		St=1;
		rst=1;
		#10;
		St=0;
		rst=1;
		#320;
		#10;
		St=1;
		rst=1;
		Multiplicador = 13;
		Multiplicando = 10;
		#10;
		St=0;
		#320;
		#10;
		St=1;
		rst=1;
		Multiplicador = 65535;
		Multiplicando = 65535;
		#10;
		St=0;
		#320;
		#10;
		St=1;
		rst=1;
		Multiplicador = 3;
		Multiplicando = 15;
		#10;
		St=0;
		#320;
		#10;
		St=1;
		rst=1;
		Multiplicador = 1;
		Multiplicando = 1;
		#10;
		St=0;
		#320;
		
	end
	
	initial #4000 $stop;
	
	

endmodule